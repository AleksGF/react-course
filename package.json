    "prettier": "3.0.3",
    "react-redux": "^8.1.3",
    "sass": "^1.69.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "undici": "^5.27.2",
    "vite": "^4.4.5"
